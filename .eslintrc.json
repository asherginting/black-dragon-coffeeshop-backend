{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "semi": ["error", "always"],
      "eqeqeq": 0,
      "no-unused-vars": 0,
      "quotes": 0,
    }
}
